{
	"name": "My Bookmarks",
	"description": "方便隐藏书签栏后，还能使用书签打开网页",
	"version": "1.3.1",
	"incognito": "split",
	"browser_action": {
		"default_popup": "index.html"
	},
	"permissions": [
		"bookmarks",
		"tabs",
		"chrome://favicon/",
		"http://bookmarks.oneo.me/"
	],
	"icons": {
		"16": "res/icon16.png",
		"19": "res/icon19.png",
		"48": "res/icon48.png",
		"64": "res/icon64.png"
	},
	"manifest_version": 2
}